package db.manager;

public class Device {
	private int id;
	private String tagName="";
	private String ip="";
	private String companyId="";
	private String description="";
	private boolean enable=false;
	private String  mac="";
	private String ucl="";
	private String lcl="";
	private String ucl02="";
	private String lcl02="";
	private String tagType="";
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getTagName() {
		return tagName;
	}
	public void setTagName(String tagName) {
		this.tagName = tagName;
	}
	public String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public String getCompanyId() {
		return companyId;
	}
	public void setCompanyId(String companyId) {
		this.companyId = companyId;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public boolean isEnable() {
		return enable;
	}
	public void setEnable(boolean enable) {
		this.enable = enable;
	}
	public String getMac() {
		return mac;
	}
	public void setMac(String mac) {
		this.mac = mac;
	}
	public String getUcl() {
		return ucl;
	}
	public void setUcl(String ucl) {
		this.ucl = ucl;
	}
	public String getLcl() {
		return lcl;
	}
	public void setLcl(String lcl) {
		this.lcl = lcl;
	}
	public String getUcl02() {
		return ucl02;
	}
	public void setUcl02(String ucl02) {
		this.ucl02 = ucl02;
	}
	public String getLcl02() {
		return lcl02;
	}
	public void setLcl02(String lcl02) {
		this.lcl02 = lcl02;
	}
	public String getTagType() {
		return tagType;
	}
	public void setTagType(String tagType) {
		this.tagType = tagType;
	}
	@Override
	public String toString() {
		return "Device [id=" + id + ", tagName=" + tagName + ", ip=" + ip + ", companyId=" + companyId
				+ ", description=" + description + ", enable=" + enable + ", mac=" + mac + ", ucl=" + ucl + ", lcl="
				+ lcl + ", ucl02=" + ucl02 + ", lcl02=" + lcl02 + ", tagType=" + tagType + "]";
	}
		
}
