
public class TestCaculatortest {
	
	public static void main(String[] args){
		
		Caculatortest mycac = new Caculatortest();
		
		int sum1 = mycac.sum(1, 2);
		System.out.println(sum1);
		
		float sum2 = mycac.sum(3.1f, 4.2f);
		System.out.println(sum2);
		
		double sum3 = mycac.sum(3.2, 4.2);
		System.out.println(sum3);
		
		
		
		
	}
	

}
