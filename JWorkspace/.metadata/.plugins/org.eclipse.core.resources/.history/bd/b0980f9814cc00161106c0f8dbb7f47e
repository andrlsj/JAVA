
public class MyArrayTest {

	public static void main(String[] args) {
		
		int[][]x = {
				{1,2,3},{4,5,6},{7,8,9}
		};
		int[][]y ={
				{1,1,1},{2,2,2},{3,3,3}
		};
		int[][]z = {
				{1,1,1},{2,2,2},{1,1,1}
		};
		
		int sum = 0;
		for(int i = 0; i<x.length;i++){
			
			for(int j =0; j<x.length;j++){
			
				sum = sum + x[i][j];
			}
		}
	
		System.out.println(sum);
	
	
	
	
	
	
	
	
	
	
	
	}
}
