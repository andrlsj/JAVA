
public enum MyGender {

	MALE,FEMALE;
	//public String name;
	
	private String name;
	
	public void setName(String name){
		
		switch(this){
		
		case MALE:
			if(name.equals("男")){
				this.name = name;
			}else{
				System.out.println("wrong");
			}
		
		case FEMALE:
			if(name.equals("女")){
				this.name= name;
			}else{
				System.out.println("wrong");
				}		
			}
		
		}
	
	public String getName(){
		return this.name();
		
	}
	
	
	
	
}
